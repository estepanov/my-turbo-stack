{
  "name": "@myturbostack/auth",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./env": {
      "require": "./dist/env.cjs",
      "import": "./dist/env.js",
      "types": "./dist/env.d.ts"
    },
    "./options": {
      "require": "./dist/options.cjs",
      "import": "./dist/options.js",
      "types": "./dist/options.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "jest": {
    "preset": "@myturbostack/jest-presets/jest/node"
  },
  "peerDependencies": {
    "next-auth": "^4.24.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@myturbostack/jest-presets": "workspace:*",
    "@myturbostack/tsconfig": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.9.2",
    "next-auth": "^4.24.7",
    "zod": "^3.22.4"
  }
}
